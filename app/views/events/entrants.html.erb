<div id='event-entrant-index'>
  <div class="row">
    <div class="span9">
      <h2>List Entrants for Event: <%=@event.name%></h2>
        <ul>
          <% @entrants.each do |e| %>
            <li>
              <%= text_field_tag '', e.full_name, :class => 'span3 uneditable-input' %>
            </li>
          <% end %>
        </ul>
    </div>
  </div>
</div>
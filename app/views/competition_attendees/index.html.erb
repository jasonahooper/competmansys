<div id='competition-attendee-index'>
  <div class="row">
    <div class="span9">
      <h2>List Attendees for Competition: <%=@comp.name%></h2>
        <ul>
          <% @attendees.each do |a| %>
            <li>
              <%= "#{a.first_name} #{a.last_name} #{a.email}" %>
            </li>
          <% end %>
        </ul>
    </div>
  </div>
</div>